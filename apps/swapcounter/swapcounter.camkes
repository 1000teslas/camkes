import <std_connector.camkes>;

procedure Swapper {
  unsigned int swap(inout int a, inout int b);
}

component Client { control; uses Swapper cs; }

component Service {  provides Swapper ss;  }

assembly {
  composition {
    component Client c;
    component Service s;
    connection seL4RPCSimple conn(from c.cs, to s.ss);
  }
}
