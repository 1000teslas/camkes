import <std_connector.camkes>;

component Foo {
    control;
    dataport Buf d1;
    dataport Buf d2;
    dataport Buf d3;
    dataport Buf d4;
}

assembly {
    composition {
        component Foo f;

        connection seL4SharedData conn(from f.d1, from f.d2, to f.d3, to f.d4);
    }
}
