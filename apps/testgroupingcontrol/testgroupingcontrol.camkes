/* @TAG(NICTA_BSD) */

import <std_connector.camkes>;

procedure Ping {
    void ping(void);
}

component Foo {
    control;
    uses Ping p;
}

component Bar {
    provides Ping p;
}

assembly {
    composition {
        group f {
            component Foo f1;
            component Foo f2;
        }
        component Bar b;
        connection seL4RPCCall conn(from f.f1.p, from f.f2.p, to b.p);
    }
}
