import <std_connector.camkes>;

procedure Foo {
    void foo(void);
}

component Client {
    control;
    uses Foo f;
    uses Foo g;
}

component Server {
    provides Foo f;
}

assembly {
    composition {
        component Client c;
        component Server s;
        connection seL4RPCCall conn(from c.f, from c.g, to s.f);
    }
}
