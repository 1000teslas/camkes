import <std_connector.camkes>;

procedure Dummy {}

component Foo {
    control;
    provides Dummy d1;
    uses Dummy d2;
}

assembly {
    composition {
        component Foo foo;
        connection seL4RPC conn(from foo.d2, to foo.d1);
    }
}
